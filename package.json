{
  "name": "aiuta-web-sdk",
  "version": "0.0.94",
  "private": true,
  "type": "module",
  "scripts": {
    "fmt": "prettier . --check",
    "fmt:fix": "prettier . --write",
    "lint": "eslint --report-unused-disable-directives && tsc --noEmit",
    "lint:fix": "eslint --fix",
    "dev": "vite --config vite.config.iframe.ts",
    "build:iframe": "vite build --config vite.config.iframe.ts",
    "build:sdk:debug": "vite build --config vite.config.sdk.ts --mode debug",
    "build:sdk:dev": "vite build --config vite.config.sdk.ts --mode dev",
    "build:sdk:preprod": "vite build --config vite.config.sdk.ts --mode preprod",
    "build:sdk:strict": "vite build --config vite.config.sdk.ts --mode strict",
    "build:sdk": "vite build --config vite.config.sdk.ts",
    "build:debug": "npm run build:iframe && npm run build:sdk:debug",
    "build:dev": "npm run build:iframe && npm run build:sdk:dev",
    "build:preprod": "npm run build:iframe && npm run build:sdk:preprod",
    "build:strict": "npm run build:iframe && npm run build:sdk:strict",
    "build": "npm run build:iframe && npm run build:sdk",
    "deploy:docs": "cp -rf ./dist/sdk/index.umd.js ../docs/docs/js/ && rm -rf ../docs/docs/iframe && cp -rf ./dist/iframe ../docs/docs",
    "local": "npm run lint && npm run build:debug && npm run deploy:docs"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.6.1",
    "@types/uuid": "^10.0.0",
    "bowser": "^2.12.1",
    "dayjs": "^1.11.18",
    "formidable": "^3.5.4",
    "framer-motion": "^12.23.12",
    "next-qrcode": "^2.5.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.8.0",
    "tslib": "^2.8.1",
    "uuid": "^12.0.0"
  },
  "devDependencies": {
    "@types/formidable": "^3.4.5",
    "@types/node": "^24.2.0",
    "@types/react": "^19.0.11",
    "@types/react-dom": "^19.0.11",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.35.0",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.90.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
