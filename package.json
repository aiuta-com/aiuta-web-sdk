{
  "name": "aiuta-web-sdk",
  "version": "2.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "// ğŸ” ": "Code Quality",
    "fmt": "prettier . --check",
    "fmt:fix": "prettier . --write",
    "lint": "eslint --report-unused-disable-directives && tsc --noEmit",
    "lint:fix": "eslint --fix",
    "// ğŸ“± ": "Build App",
    "build:app:debug": "vite build --config vite.config.app.ts --mode debug",
    "build:app:dev": "vite build --config vite.config.app.ts --mode dev",
    "build:app:preprod": "vite build --config vite.config.app.ts --mode preprod",
    "build:app": "vite build --config vite.config.app.ts",
    "// ğŸ“¦ ": "Build SDK",
    "build:sdk:debug": "vite build --config vite.config.sdk.ts --mode debug",
    "build:sdk:dev": "vite build --config vite.config.sdk.ts --mode dev",
    "build:sdk:preprod": "vite build --config vite.config.sdk.ts --mode preprod",
    "build:sdk": "vite build --config vite.config.sdk.ts",
    "// ğŸ—ï¸ ": "Build All",
    "build:debug": "npm run build:app:debug && npm run build:sdk:debug",
    "build:dev": "npm run build:app:dev && npm run build:sdk:dev",
    "build:preprod": "npm run build:app:preprod && npm run build:sdk:preprod",
    "build": "npm run build:app && npm run build:sdk",
    "// ğŸš€ ": "Development",
    "local:debug": "vite --config vite.config.app.ts --mode debug",
    "local:deploy": "cp -rf ./dist/sdk/index.umd.js ../docs/docs/js/ && rm -rf ../docs/docs/app && cp -rf ./dist/app ../docs/docs",
    "local:build": "npm run build:debug && npm run local:deploy && npm run local:debug",
    "local": "npm run lint && npm run local:build"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.6.1",
    "@tanstack/react-query": "^5.90.10",
    "@types/uuid": "^10.0.0",
    "bowser": "^2.12.1",
    "dayjs": "^1.11.18",
    "formidable": "^3.5.4",
    "next-qrcode": "^2.5.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.8.0",
    "tslib": "^2.8.1",
    "uuid": "^12.0.0"
  },
  "devDependencies": {
    "@types/formidable": "^3.4.5",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/node": "^24.2.0",
    "@types/react": "^19.0.11",
    "@types/react-dom": "^19.0.11",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.35.0",
    "html-minifier-terser": "^7.2.0",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.90.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
